    <p>
    <div class="container">  <!-- container class 가 페이지의 컨텐츠영역을 지정 -->
    <div class="jumbotron" style="width:1000px; margin:auto">
      <%
      var idata = idata[0];
      %>
      <div class="input-group-prepend">
        <span class="input-group-text"style="font-size:1em; font-weight:bold;">제&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;목</span>
        <div class="d-inline-flex p-2 bd-highlight"style="color: black; font-size: 1em; font-weight: bold;">&nbsp;:&nbsp;<%=idata.ititle %></div>
      </div>
      <div class="starter-template">
        <h4> </h4> <hr>
      </div>

   <form name=join onsubmit="return validate()" method=get>
     <div class="input-group-prepend">
       <span class="input-group-text" style="font-size:1em; font-weight:bold;">작&nbsp;&nbsp;성&nbsp;&nbsp;자</span>
       <div class="d-inline-flex p-2 bd-highlight" style="color: black; font-size: 1em; font-weight: bold;">&nbsp;:&nbsp;<%=idata.uid %></div>
       <div class="text-right"></div>
     </div>
     <div class="starter-template">
       <h4> </h4> <hr>
     </div>
     <div class="input-group-prepend">
       <span class="input-group-text"style="font-size:1em; font-weight:bold;">문의유형</span>
       <div class="d-inline-flex p-2 bd-highlight"style="color: black; font-size: 1em; font-weight: bold;">&nbsp;:&nbsp;<%=idata.itype %></div>
       <div class="text-right"></div>
     </div>
     <div class="starter-template">
       <h4> </h4> <hr>
     </div>
     <div class="input-group-prepend">
       <span class="input-group-text"style="font-size:1em; font-weight:bold;">내용</span>
        <div>
          <textarea name="content" cols="78" rows="11" readonly style="color: black; font-size: 1.5em;"><%=idata.iquestion%></textarea>
        </div>
     </div>
     <div class="starter-template">
       <h4> </h4> <hr>
     </div>
     <div class="input-group-prepend">
       <span class="input-group-text"style="font-size:1em; font-weight:bold;">답변</span>
        <div>
          <textarea name="content" cols="78" rows="11" readonly style="color: black; font-size: 1.5em;"><%=idata.ireply%></textarea>
        </div>
     </div>
     <div class="starter-template">
       <h4> </h4> <hr>
     </div>
         <a class="btn btn-secondary btn-lg btn-block" href="/adminuser/boardlist" style="font-size:1em; font-weight:bold;">돌아가기</a>
      </form>
     </div>
     </div>


