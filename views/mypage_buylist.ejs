<form name=join onsubmit="return validate()" method=post action="/users/buylist?_method=PUT">

  <div class="row">
    <div class="col-md-4 order-md-2 mb-4">
      <h4 class="d-flex justify-content-between align-items-center mb-3">
        <span class="text-muted"></span>
        <span class="badge badge-secondary badge-pill"></span>
      </h4>
      <ul class="list-group mb-3">
        <li class="list-group-item d-flex justify-content-between lh-condensed">
          <div>
            <h6>내 정보 수정</h6>
          </div>
          <a class="btn btn-sm btn-light" href="/users/update">이동하기</a>
        </li>
        <li class="list-group-item d-flex justify-content-between lh-condensed">
          <div>
            <h6>내 게시글 확인</h6>
          </div>
          <a class="btn btn-sm btn-light" href="/users/board/1">이동하기</a>
        </li>
        <li class="list-group-item d-flex justify-content-between lh-condensed">
          <div>
            <h6>구매 내역</h6>
          </div>
          <a class="btn btn-sm btn-light" href="/users/buylist">이동하기</a>
        </li>
        <li class="list-group-item d-flex justify-content-between lh-condensed">
          <div>
            <h6>튜터 등록</h6>
          </div>
          <a class="btn btn-sm btn-light" href="/users/apptutor">이동하기</a>
        </li>
        <li class="list-group-item d-flex justify-content-between bg-light">
          <div class="text-success">
            <h6>재능 관리</h6>
            <small>튜터만 이용 가능합니다.</small>
          </div>
          <a class="btn btn-sm btn-success" href="/users/talmanage/1">이동하기</a>
        </li>
        <li class="list-group-item d-flex justify-content-between bg-light">
          <div class="text-success">
            <h6>판매 내역</h6>
            <small>튜터만 이용 가능합니다.</small>
          </div>
          <a class="btn btn-sm btn-success" href="/users/selllist/1">이동하기</a>
        </li>
        <li class="list-group-item d-flex justify-content-between lh-condensed">
          <div>
            <h6>회원 탈퇴</h6>
          </div>
          <a class="btn btn-sm btn-light" href="/users/userdel">이동하기</a>
        </li>
      </ul>
    </div>

    <div class="col-xs-8 order-md-1">
      <div class="row">
      <table class="table table-hover">
        <thead>
          <tr>
            <th> Check</th>
            <th> 판매자명</th>
            <th> 재능명</th>
            <th> 구매날짜</th>
            <th> 가격(원)</th>
            <th> 거래상태</th>
          </tr>
        </thead>
        <tbody>
          <% buydata.forEach(function (item, index) { %>
          <tr>
            <td> <input type = "checkbox", name = "dnum", value = <%= item.dnum %>></td>
            <td> <%= item.dseller %> </td>
            <td> <%= item.dname %> </td>
            <td> <%= item.ddate.toLocaleDateString(); %> </td>
            <td> <%= item.dprice %> </td>
            <td> <%= item.dcomplete %> </td>
          </tr>
          <% }); %>
       </table>
       <button class="btn btn-primary btn-lg btn-block" type="submit">거래 완료하기</button>
       </div>
        </div>
    </div>
  </div>
  </form>
